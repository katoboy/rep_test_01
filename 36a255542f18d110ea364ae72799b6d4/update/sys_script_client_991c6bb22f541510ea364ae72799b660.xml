    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>RW比率(Read)の値をもとにRW比率(Write)を自動計算する（10からRW比率(Read)の値を引いた値）。</description>
        <field>u_04_rw_rate_read</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>camdev_AutoSetWrRateWrite_OnChange</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading) {
        return;
    }

    var value = '';
    if (newValue == '') {
        g_form.setValue('u_04_rw_rate_write', value);
    } else {
        value = 10 - parseInt(newValue, 10);
        g_form.setValue('u_04_rw_rate_write', value);
    }
}]]></script>
